{
	"$schema": "https://schemas.wp.org/wp/6.6/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#f9f9f9",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#555555",
					"name": "Body Text",
					"slug": "body-text"
				},
				{
					"color": "#f1f1f1",
					"name": "Section Background",
					"slug": "section-bg"
				},
				{
					"color": "#1f1d1e",
					"name": "Headings",
					"slug": "heading"
				},
				{
					"color": "#cf4a46",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#1f1d1d",
					"name": "Secondary",
					"slug": "secondary"
				}
			]
		},
		"layout": {
			"contentSize": "999px",
			"wideSize": "1170px"
		},
		"spacing": {
			"units": [
				"px",
				"%",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Poppins",
							"fontStretch": "",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/Poppins-Regular.ttf"
							]
						}
					],
					"fontFamily": "\"Poppins\", sans-serif",
					"name": "Poppins",
					"slug": "poppins-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Kumbh Sans",
							"fontStretch": "",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/KumbhSans-Regular.ttf"
							]
						},
						{
							"fontFamily": "Kumbh Sans",
							"fontStretch": "",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/KumbhSans-Medium.ttf"
							]
						},
						{
							"fontFamily": "Kumbh Sans",
							"fontStretch": "",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/KumbhSans-SemiBold.ttf"
							]
						},
						{
							"fontFamily": "Kumbh Sans",
							"fontStretch": "",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/KumbhSans-Bold.ttf"
							]
						}
					],
					"fontFamily": "\"Kumbh Sans\", sans-serif",
					"name": "Kumbh Sans",
					"slug": "kumbh-font"
				}
			],
			"fontSizes": [
				{
					"name": "Body Text",
					"size": "15px",
					"slug": "body-text"
				},
				{
					"name": "Extra Small",
					"size": "14px",
					"slug": "extra-small"
				},
				{
					"name": "Small",
					"size": "16px",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "18px",
					"slug": "medium"
				},
				{
					"name": "Regular",
					"size": "22px",
					"slug": "regular"
				},
				{
					"name": "Large",
					"size": "26px",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "32px",
					"slug": "extra-large"
				},
				{
					"name": "Big",
					"size": "clamp(2.063rem, 1.759rem + 1.518vw, 3.125rem)",
					"slug": "big"
				},
				{
					"name": "Huge",
					"size": "clamp(40px, calc(2.25rem + ((1vw - 3.2px) * 2.375)), 74px);",
					"slug": "huge"
				},
				{
					"name": "Gigantic",
					"size": "clamp(42px, calc(2.625rem + ((1vw - 3.2px) * 2.75)), 86px);",
					"slug": "gigantic"
				},
				{
					"name": "Banner Title",
					"size": "clamp(3rem, 2.781rem + 1.094vw, 3.875rem)",
					"slug": "banner-title"
				},
				{
					"name": "Section Title",
					"size": "clamp(2rem, 1.813rem + 0.938vw, 2.75rem)",
					"slug": "section-title"
				}
			],
			"fontWeight": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/navigation": {
				"color": {
					"text": "var(--wp--preset--color--white);"
				},
				"elements": {
					"link": {
						":active": {
							"color": {
								"text": "var(--wp--preset--color--primary);"
							}
						},
						":focus": {
							"color": {
								"text": "var(--wp--preset--color--primary);"
							}
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--primary);"
							}
						}
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--kumbh-font)",
					"fontSize": "14px",
					"fontWeight": "600",
					"letterSpacing": "2px"
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--secondary);"
						},
						"typography": {
							"textDecoration": "none",
							"textTransform": "capitalize"
						}
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--kumbh-font)",
					"fontSize": "clamp(1.75rem, 1.619rem + 0.656vw, 2.25rem);",
					"fontWeight": "700",
					"lineHeight": "1.2"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--body-text)"
		},
		"elements": {
			"button": {
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--secondary)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--white)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "50px",
					"lineHeight": "1.5"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "43px",
					"lineHeight": "1.3"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "25px"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "22px"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "20px"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "18px"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--heading)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--kumbh-font)",
					"fontWeight": "700"
				}
			},
			"link": {
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					},
					"outline": {
						"style": "dotted",
						"width": "1px"
					}
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--heading)"
				}
			}
		},
		"spacing": {
			"margin": {
				"bottom": "0",
				"left": "0",
				"right": "0",
				"top": "0"
			},
			"padding": {
				"bottom": "0",
				"left": "0",
				"right": "0",
				"top": "0"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--poppins-font)",
			"fontSize": "var(--wp--preset--font-size--body-text)",
			"fontWeight": "400",
			"lineHeight": "1.5rem"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"title": "Lite",
	"version": 3
}